<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <!-- background like layout -->
    <item android:id="@+id/background">
        <shape android:shape="rectangle">
            <size
                android:width="50dp"
                android:height="45dp" />
            <solid android:color="#FFFFFF" />
        </shape>
    </item>

    <!-- shirt -->
    <item android:id="@+id/shirt"
        android:right="12.5dp"
        android:left="12.5dp">
        <shape android:shape="rectangle">
            <solid android:color="#FAD55C" />
        </shape>
    </item>

    <!-- left sleeve -->
    <item android:id="@+id/left_sleeve"
        android:right="30dp"
        android:left="5dp"
        android:bottom="27.5dp">
        <rotate
            android:fromDegrees="65">
            <shape android:shape="rectangle">
                <solid android:color="#FAD55C" />
            </shape>
        </rotate>
    </item>

    <!-- right sleeve -->
    <item android:id="@+id/right_sleeve"
        android:right="5dp"
        android:left="30dp"
        android:bottom="27.5dp">
        <rotate
            android:fromDegrees="-65">
            <shape android:shape="rectangle">
                <solid android:color="#FAD55C" />
            </shape>
        </rotate>
    </item>

    <!-- cleavage -->
    <item android:id="@+id/cleavage"
        android:right="16.5dp"
        android:left="16.5dp"
        android:top="-16.5dp"
        android:bottom="38.5dp">
        <shape android:shape="oval">
            <solid android:color="#FFFFFF" />
        </shape>
    </item>
</layer-list>